{
  "OidcConfiguration": {
    "Authority": "https://tsp-dev-idp-server.azurewebsites.net", //IDP 
    "ClientId": "tsp",
    "DefaultScopes": [
      "openid",
      "profile",
      "email",
      "tspapi",
      "roleclaim"
